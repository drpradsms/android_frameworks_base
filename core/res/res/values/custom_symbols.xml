<resources>
    <!-- True if the notifications should dynamically tint the app icon -->
    <java-symbol type="bool" name="config_allowNotificationIconTinting" />

    <!-- Notification icon tint -->
    <java-symbol type="color" name="notification_icon_default_color" />

    <!-- Ambient notification icon tint -->
    <java-symbol type="color" name="notification_ambient_icon_default_color" />

    <!-- True if the notification's dynamic app icon tint should be optimised for dark
         backgrounds -->
    <java-symbol type="bool" name="config_useDarkBgNotificationIconTinting" />

    <!-- Notification info -->
    <java-symbol type="color" name="notification_info_button_text" />
    <java-symbol type="bool" name="config_useDarkBgNotificationIconTextTinting" />

    <!-- Whether device has oled display -->
    <java-symbol type="bool" name="config_hasOledDisplay" />

    <!-- Custom accent color -->
    <java-symbol type="color" name="custom_accent_color" />
</resources>
